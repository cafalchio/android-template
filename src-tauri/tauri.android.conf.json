{
    "$schema": "https://schema.tauri.app/config/2",
    "productName": "android-template",
    "version": "0.1.0",
    "identifier": "com.android-template.app",
    "build": {
    "beforeDevCommand": "npm run dev",
    "devUrl": "http://localhost:1420",
    "beforeBuildCommand": "npm run build",
    "frontendDist": "../dist"
    },
    "bundle": {
      "active": true,
      "targets": "all",
    "icon": [
        "icons/32x32.png",
        "icons/128x128.png",
        "icons/128x128@2x.png",
        "icons/icon.icns",
        "icons/icon.ico"
      ],
      "android": {
        "minSdkVersion": 24
      }
    },
    "app": {
      "windows": [
        {
          "title": "Template",
          "width": 800,
          "height": 600,
          "resizable": false,
          "fullscreen": true,
          "transparent": true,
          "decorations": false
        }
      ],
      "security": {
        "csp": null
      }
    },
    "plugins": {
      "statusBar": {
        "backgroundColor": "transparent",
        "style": "light"
      }
    }
  }